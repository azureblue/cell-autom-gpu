{
    "ca": {
        "buffer": {
            "width": 2048,
            "height": 1024
        },

        "view": {
            "scale": 2,
            "centerX": 256,
            "centerY": 256
        },

        "room": {
            "width": 256,
            "height": 256,
            "rule": "B3/S23",
            "separate": true
        }
    },

    "initialState": [{
            "roomX": 0,
            "roomY": 0,
            "dataset": "random256x256"
        }, {
            "roomX": 1,
            "roomY": 0,
            "dataset": "random256x256"
        }, {
            "roomX": 0,
            "roomY": 1,
            "dataset": "checker256x256"
        }, {
            "roomX": 1,
            "roomY": 1,
            "dataset": "checker256x256"
        }
    ],

    "datasets": [{
            "name": "checker256x256",
            "width": 256,
            "height": 256,
            "format": "bits",
            "url": "checker256.bits"
        }, {
            "name": "random256x256",
            "width": 256,
            "height": 256,
            "format": "random"
        }]
}
